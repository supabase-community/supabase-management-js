{
  "name": "supabase-management-js",
  "version": "1.0.0",
  "description": "Convenience wrapper for the Supabase Management API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup --dts-resolve",
    "typecheck": "tsc --noEmit",
    "generate:spec": "node scripts/fetch-spec.mjs && node scripts/patch-spec.mjs",
    "generate:client": "orval --config orval.config.ts && node scripts/generate-index.mjs",
    "generate": "npm run generate:spec && npm run generate:client",
    "changeset:version": "changeset version",
    "changeset:release": "npm run build && changeset publish"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "25",
    "esbuild": "^0.27.3",
    "orval": "^8.4.0",
    "rimraf": "^6.1.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}